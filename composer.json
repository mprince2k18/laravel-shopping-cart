{
    "name": "mprince/laravelshoppingcart",
    "description": "Laravel Shopping Cart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl",
            "homepage": "http://robgloudemans.nl/"
        },
        {
            "name": "Mohammad Prince",
            "email": "mprince2k16@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3||^8.0.0",
        "laravel/framework": "^8.0.0||^9.0.0",
        "nunomaduro/collision": "^6.1",
        "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "illuminate/session": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "illuminate/events": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "nesbot/carbon": "~1.20||^1.26.3||^2.0",
        "spatie/laravel-ignition": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0||^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0|^6.0|^7.4"
    },
    "autoload": {
        "psr-4": {
            "Mprince\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mprince\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "Mprince/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mprince\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Mprince\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
